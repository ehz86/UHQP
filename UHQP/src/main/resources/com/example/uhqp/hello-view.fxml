                  <MediaView fx:id="mediaView" fitHeight="720.0" fitWidth="1080.0" StackPane.alignment="CENTER" />
               </children>
            </StackPane>
         </center>
         <bottom>
            <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="728.0" style="-fx-background-color: #D0E1F9;" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="BackButton" contentDisplay="CENTER" mnemonicParsing="false" onAction="#BackButtonPressed" prefHeight="40.0" prefWidth="40.0">
