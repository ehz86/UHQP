                        <Insets left="2.0" right="2.0" top="25.0" />
                     </HBox.margin>
                     <graphic>
                        <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@images/skipleft.png" />
                           </image>
                        </ImageView>
                     </graphic></Button>
                  <Button fx:id="PlayButton" mnemonicParsing="false" onAction="#PlayButtonPressed" prefHeight="50.0" prefWidth="50.0">
                     <HBox.margin>
                        <Insets left="2.0" right="2.0" top="20.0" />
                     </HBox.margin>
                     <graphic>
                        <ImageView fitHeight="40.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@images/play.png" />
                           </image>
                        </ImageView>
                     </graphic></Button>
                  <Button fx:id="ForwardButton" mnemonicParsing="false" onAction="#ForwardButtonPressed" prefHeight="40.0" prefWidth="40.0">
